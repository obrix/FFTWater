# - Config file for the FFTWater package
# It defines the following variables
#  FFTWater_INCLUDE_DIRS - include directories for FooBar
#  FFTWater_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(FFTWater_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(FFTWater_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET foo AND NOT FFTWater_BINARY_DIR)
  include("${FFTWater_CMAKE_DIR}/FFTWaterTargets.cmake")
endif()

# These are IMPORTED targets created by FFTWaterTargets.cmake
set(FFTWater_LIBRARIES OceanSurface)
